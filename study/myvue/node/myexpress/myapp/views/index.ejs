<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <link rel="stylesheet" href="/css/bootstrap.css">
    <title>Document</title>
</head>
<body>
<div class="container">
<table  class="table ">
    <caption style="width:100%;height: 50px;background:#ccc;margin: 0 auto;text-align: center;font-size: 20px">MMP</caption>
    <tr class="info">
        <th>id</th>
        <th>姓名</th>
        <th>年龄</th>
        <th>性别</th>
        <th>操作</th>
    </tr>
    <% for(var i=0; i<data.length; i++) {%>


    <tr class="success">
        <td><%= data[i].id %></td>
        <td><%= data[i].name %></td>
        <td><%= data[i].age %></td>
        <td><%= data[i].sex %></td>
        <td>
            <a href="/updata/<%=data[i].id%>">编辑</a>
            <a href="/del/<%=data[i].id%>">删除</a>
        </td>
    </tr>
    <% } %>


</table>
    <a href="/add" class="glyphicon glyphicon-plus btn btn-primary btn-lg active" style="">添加</a>
</div>
</body>
</html>